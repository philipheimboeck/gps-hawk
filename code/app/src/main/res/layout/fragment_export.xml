<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="gps.fhv.at.gps_hawk.activities.fragments.ExportFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/activity_horizontal_margin"
            android:text="@string/amount_of_captured_waypoints"/>

        <TextView
            android:id="@+id/tbx_amount_of_waypoints"
            android:layout_width="match_parent"
            android:layout_height="@dimen/activity_horizontal_margin"/>

        <Button
            android:id="@+id/button_do_export"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:text="@string/start_export"/>


    </LinearLayout>
